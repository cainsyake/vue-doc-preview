{
  "name": "vue-doc-preview",
  "version": "0.3.2",
  "description": "A vue document preview component",
  "keywords": [
    "document",
    "markdown",
    "preview"
  ],
  "main": "./dist/vueDocPreview.umd.min.js",
  "author": "cainsyake(cainsyake@qq.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/cainsyake/vue-doc-preview.git"
  },
  "bugs": {
    "url": "https://github.com/cainsyake/vue-doc-preview/issues",
    "email": "cainsyake@qq.com"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-lib": "vue-cli-service build --target lib --name vueDocPreview ./src/index.js",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service serve ./test/index.js",
    "build": "vue-cli-service build ./test/index.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "highlight.js": "^9.12.0",
    "marked": "^4.0.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.11",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.11",
    "@vue/cli-service": "^3.0.0-rc.11",
    "@vue/eslint-config-standard": "^3.0.0-rc.11",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "iview": "^3.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "files": [
    "dist/",
    "src/",
    "babel.config.js",
    "vue.config.js",
    "yarn.lock",
    "LICENSE",
    "README-CN.md",
    "README.md"
  ],
  "license": "MIT"
}
